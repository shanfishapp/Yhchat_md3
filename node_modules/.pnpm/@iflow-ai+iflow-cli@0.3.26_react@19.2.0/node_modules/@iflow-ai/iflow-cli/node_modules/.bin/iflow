#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/md3/node_modules/.pnpm/@iflow-ai+iflow-cli@0.3.26_react@19.2.0/node_modules/@iflow-ai/iflow-cli/bundle/node_modules:/data/data/com.termux/files/home/md3/node_modules/.pnpm/@iflow-ai+iflow-cli@0.3.26_react@19.2.0/node_modules/@iflow-ai/iflow-cli/node_modules:/data/data/com.termux/files/home/md3/node_modules/.pnpm/@iflow-ai+iflow-cli@0.3.26_react@19.2.0/node_modules/@iflow-ai/node_modules:/data/data/com.termux/files/home/md3/node_modules/.pnpm/@iflow-ai+iflow-cli@0.3.26_react@19.2.0/node_modules:/data/data/com.termux/files/home/md3/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/md3/node_modules/.pnpm/@iflow-ai+iflow-cli@0.3.26_react@19.2.0/node_modules/@iflow-ai/iflow-cli/bundle/node_modules:/data/data/com.termux/files/home/md3/node_modules/.pnpm/@iflow-ai+iflow-cli@0.3.26_react@19.2.0/node_modules/@iflow-ai/iflow-cli/node_modules:/data/data/com.termux/files/home/md3/node_modules/.pnpm/@iflow-ai+iflow-cli@0.3.26_react@19.2.0/node_modules/@iflow-ai/node_modules:/data/data/com.termux/files/home/md3/node_modules/.pnpm/@iflow-ai+iflow-cli@0.3.26_react@19.2.0/node_modules:/data/data/com.termux/files/home/md3/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bundle/iflow.js" "$@"
else
  exec node  "$basedir/../../bundle/iflow.js" "$@"
fi
